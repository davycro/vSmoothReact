{
  "name": "electronhello",
  "version": "1.0.0",
  "description": "Learning to make a barebones electron and react application",
  "main": "main.js",
  "scripts": {
    "test": "test",
    "dev": "NODE_ENV=dev nodemon --watch ./app/**/* --watch . --ext 'css,html,js,json'  --exec 'npm run babel && electron .'",
    "babel": "babel ./app --out-dir ./dist --copy-files"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "readme"
    ]
  },
  "author": "David M Crockett",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "babel-plugin-styled-components": "^1.10.6",
    "electron": "^7.2.4",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.1",
    "react-slider": "^1.0.1",
    "styled-components": "^4.4.1"
  }
}
